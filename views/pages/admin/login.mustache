<!-- Main start -->
<main class="container my-5 py-5 h-100 d-flex align-items-center">
    <div class="row no-gutters w-100">
        <div class="col-11 col-md-5 col-lg-3 mx-auto ">
            <h3 class="text-center">Enter to apanel</h3>
            <form action="" method="POST">
                <div class="form-group">
                    <label for="login">Login</label>
                    <div class="input-group mb-2 mr-sm-2">
                        <div class="input-group-prepend">
                            <div class="input-group-text"><i class="fas fa-user"></i></div>
                        </div>
                        <input class="form-control" type="text" id="login" value="admin" name="login" required>
                    </div>

                </div>
                <div class="form-group">
                    <label for="password">Password</label>
                    <div class="input-group mb-2 mr-sm-2">
                        <div class="input-group-prepend">
                            <div class="input-group-text"><i class="fas fa-key"></i></div>
                        </div>
                        <input class="form-control" type="password" id="password" value="admin" name="password" required>
                    </div>
                </div>
                <div class="text-center">
                    <button class="btn btn-primary btn-block btn-lg" type="submit">Enter</button>
                </div>
            </form>
        </div>
    </div>
</main>
<!-- Main end -->